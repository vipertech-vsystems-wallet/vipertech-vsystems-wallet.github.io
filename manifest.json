{
  "manifest_version": 2,
  "minimum_chrome_version": "23.0.0.0",
  "version": "0.0.7",
  "name": "VSYS GOLD WALLET",
  "short_name": "VSYS Gold Wallet",
  "description": "Fast and very simple v.systems wallet.",
  "display": "standalone",
  "theme_color": "#b99e4f",
  "background_color": "#e6e7ee",
  "browser_action": {
    "default_icon": "/images/manifest/icon-512x512.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "48": "/images/manifest/icon-48x48.png"
  }
}